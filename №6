k = int(input('введите номер квартиры '))
p = 3 # кол-во подьездов
flor = 9 # кол-во этажей
flat = 4 # кол-во квартир на этаже
c = flor * flat # кол-во квартир в подъезде
x = k // c
y = (x + 1) # в каком подъезде находится квартира
z = k % c
d = z // flat
s = (d + 1) #на каком этаже
print(str(y) + ' подъезд')
print(str(s) + ' этаж')
